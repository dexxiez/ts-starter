{
  "name": "ts-starter",
  "version": "1.0.0",
  "description": "Template for typescript projects",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts --minify --clean --sourcemap --legacy-output",
    "dev": "concurrently \"tsup src/index.ts --watch --format esm \" \"nodemon index.js\"",
    "test": "jest"
  },
  "keywords": [],
  "author": "Dexxiez",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "concurrently": "^8.2.1",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
